function Laeberkaes(int Dicke) // Dicke in mm angeben
{
  if (Dicke < 20)
  {
    echo "Des isch zwenig, dua mehr nauf!";
    return 1;
   }
   elseif
   {
    echo "Des isch guad, und no Ketchup bitte!";
    return 0;
   }
   else
   {
    echo "I brauch an Senf du Bazi!"
   }
  return 0;
}
